<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="TerminosPagosVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="xxqp.oracle.apps.ar.pdft.poplist.server.TerminosPagosVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select t.term_id   lookup_code
         ,t.name      meaning
         ,t.description 
  from ra_terms t
where trunc(sysdate) between start_date_active and nvl(end_date_active,trunc(sysdate)) order by name
   ]]></SQLQuery>
