   <SQLQuery><![CDATA[
/*select fu.user_id 
        , fu.user_name||' - '||fu.description ejecutivo
   from fnd_user fu
*/
select  lookup_code user_id
         ,description ejecutivo
   from xxqp_pdft_mgr_catalogos
where lookup_type = 'MGR_PDFT_USUARIOS'
   and nvl(status,'X') = 'Y'
   ]]></SQLQuery>
