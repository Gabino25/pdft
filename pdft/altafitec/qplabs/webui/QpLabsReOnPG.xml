                                    <oa:messageDownload id="ContratoDownload" prompt="" viewName="XxqpPdftQplabsHeaderVO1" viewAttr="ContratoFileName" contentViewAttributeName="ContratoFile" fileContentType="ContratoContentType"/>
                                 </ui:contents>
                              </oa:tableLayout>
                           </ui:contents>
                        </oa:cellFormat>
                        <oa:cellFormat id="CellFecha">
                           <ui:contents>
                              <oa:tableLayout id="TabLayFecha" hAlign="center">
