<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="PageLayoutRN" amDefName="xxqp.oracle.apps.ar.pdft.altafitec.qplabs.server.QpLabsAM" windowTitle="Detalle de Ficha Tecnica > QP Labs Re On" title="Detalle de Ficha Tecnica > QP Labs Re On" controllerClass="xxqp.oracle.apps.ar.pdft.altafitec.qplabs.webui.QpLabsReOnCO" scope="/" securityMode="selfSecured" rendered="${oa.FunctionSecurity.XXQP_PDFT_SS_QPLABS_RO_F}">
