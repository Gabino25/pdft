                                    </oa:cellFormat>
                                 </ui:contents>
                              </oa:rowLayout>
                           </ui:contents>
                        </oa:tableLayout>
                     </ui:contents>
                  </oa:contentContainer>
               </ui:contents>
            </oa:subTabLayout>
            <oa:pageButtonBar id="PageButtonBarRN">
               <ui:contents>
                  <oa:button id="CancelarFTBtn" text="Cancelar Ficha Tecnica" prompt="Cancelar Ficha Tecnica">
