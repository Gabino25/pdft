                           <ui:contents>
                              <oa:tableLayout id="TabLayNumeroFt" hAlign="center">
                                 <ui:contents>
                                    <oa:messageTextInput id="NumeroFt" prompt="Numero FT" styleClass="OraFieldText" readOnly="true" viewName="XxqpPdftQplabsHeaderVO1" viewAttr="NumeroFt" required="yes"/>
