                                       </ui:contents>
                                    </oa:cellFormat>
                                 </ui:contents>
                              </oa:rowLayout>
                           </ui:contents>
                        </oa:tableLayout>
                        <oa:tableLayout id="TabLayInfoOper2" hAlign="center">
