                              </oa:tableLayout>
                           </ui:contents>
                        </oa:cellFormat>
                        <oa:cellFormat id="CellEstatus">
                           <ui:contents>
                              <oa:tableLayout id="TabLayEstatus" hAlign="center">
                                 <ui:contents>
