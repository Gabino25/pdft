                        <oa:cellFormat id="CellEmpresaQueFactura" width="50%" hAlign="center">
                           <ui:contents>
                              <oa:tableLayout id="TabLayEmpresaQueFactura" hAlign="center">
                                 <ui:contents>
                                    <oa:messageChoice id="EmpresaQueFactura" prompt="Empresa que Factura" pickListViewDef="xxqp.oracle.apps.ar.pdft.poplist.server.EmpresaQueFacturaVO" pickListDispAttr="Meaning" pickListValAttr="LookupCode" styleClass="OraFieldText" readOnly="true" viewName="XxqpPdftQplabsHeaderVO1" viewAttr="EmpresaQueFacturaC" required="yes"/>
